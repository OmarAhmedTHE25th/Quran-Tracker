generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"

}

model SurahProgress {
  userId         String @default("")
  number         Int
  englishName    String
  completed      Boolean @default(false)
  numberOfAyahs Int  @default(0)
  completedAyahs Int     @default(0)
  @@id([userId,number])
}